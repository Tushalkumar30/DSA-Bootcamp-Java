//Given an array nums of integers, return how many of them contain an even number of digits.
class Solution {
    public int findNumbers(int[] nums) {
        int count =0;
        for(int i =0; i < nums.length; i++){
            int digit = 0;
            int n =0;
            n = nums[i];
            while( n > 0){
                n = n/10;
                digit++;
            }
            if( digit % 2==0){
                count++;
            }
        }
        return count;
    }
}   
